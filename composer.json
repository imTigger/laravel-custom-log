{
    "name": "imtigger/laravel-custom-log",
    "description": "Laravel Custom Log",
    "license": "mit",
    "authors": [
        {
            "name": "Tiger",
            "email": "tiger@tiger-workshop.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
    },
    "suggest": {
        "graylog2/gelf-php": "Required for Gelf/Graylog logging",
        "predis/predis": "Required for Redis logging"
    },
    "autoload": {
        "psr-4": {
            "Imtigger\\LaravelCustomLog\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imtigger\\LaravelCustomLog\\LaravelCustomLogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
